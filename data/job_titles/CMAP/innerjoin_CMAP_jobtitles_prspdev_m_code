let
    // Load title list (nonprofit sector)
    TitleListSource = Csv.Document(File.Contents("jobtitlepath"), [Delimiter=",", Encoding=65001, QuoteStyle=QuoteStyle.None]),
    TitleListTable = Table.PromoteHeaders(TitleListSource, [PromoteAllScalars=true]),
    TitleList = Table.SelectColumns(TitleListTable, {"title_cleaned"}),

    // Load dataset to filter (travel and tourism)
    DatasetSource = Csv.Document(File.Contents("sectorfilepath"), [Delimiter=",", Columns=7, Encoding=65001, QuoteStyle=QuoteStyle.None]),
    DatasetTable = Table.PromoteHeaders(DatasetSource, [PromoteAllScalars=true]),

    // Match rows by title_cleaned
    MatchedRows = Table.Join(DatasetTable, "title_cleaned", TitleList, "title_cleaned", JoinKind.Inner)
in
    MatchedRows
