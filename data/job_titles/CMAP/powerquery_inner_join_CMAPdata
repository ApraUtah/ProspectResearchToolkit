let
    // Load title list (nonprofit sector)
    TitleListSource = Csv.Document(File.Contents("C:\Users\u1428176\Downloads\CMAP_prospectjobtitles_nonprofitsector_2017.csv"), [Delimiter=",", Encoding=65001, QuoteStyle=QuoteStyle.None]),
    TitleListTable = Table.PromoteHeaders(TitleListSource, [PromoteAllScalars=true]),
    TitleList = Table.SelectColumns(TitleListTable, {"title_cleaned"}),

    // Load dataset to filter (education)
    DatasetSource = Csv.Document(File.Contents("C:\Users\u1428176\Downloads\jobtitlesdataset\Figure 4\dataset\titles\map\55198391_education.csv"), [Delimiter=",", Columns=7, Encoding=65001, QuoteStyle=QuoteStyle.None]),
    DatasetTable = Table.PromoteHeaders(DatasetSource, [PromoteAllScalars=true]),

    // Match rows by title_cleaned
    MatchedRows = Table.Join(DatasetTable, "title_cleaned", TitleList, "title_cleaned", JoinKind.Inner)
in
    MatchedRows
